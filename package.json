{
  "name": "superfuel",
  "version": "1.0.0",
  "description": "Web application to give fuel quote",
  "main": "app.js",
  "scripts": {
    "devServer": "live-server",
    "dev": "npm-run-all --parallel devServer watch",
    "application": "nodemon ./src/server.js",
    "watch": "sass public/sass/main.scss public/css/styles.css --watch",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
    "test": "./node_modules/.bin/mocha ./test",
    "coverage": "nyc npm run test"
  },
  "author": "Group 6",
  "license": "ISC",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.0.0-alpha.3",
    "chai": "^4.3.4",
    "mocha": "^9.0.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "parcel": "^2.0.0-beta.2",
    "parcel-bundler": "^1.12.5",
    "sass": "^1.34.1",
    "supertest": "^6.1.3",
    "validator": "^13.6.0"
  },
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "core-js": "^3.14.0",
    "dotenv": "^7.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "express-validator": "^6.12.0",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.9",
    "pug": "^3.0.2"
  }
}
